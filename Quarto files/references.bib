@article{buglioneAgricolturaAllevamentoNella2016,
  title = {{Agricoltura e allevamento nella Puglia settentrionale tra et\`a romana e Medioevo : il contributo delle bioarcheologie}},
  shorttitle = {{Agricoltura e allevamento nella Puglia settentrionale tra et\`a romana e Medioevo}},
  author = {Buglione, Antonietta and Venuto, Giovanni De and Volpe, Giuliano},
  year = {2016},
  month = may,
  journal = {M\'elanges de l'\'Ecole fran\c{c}aise de Rome - Antiquit\'e},
  number = {128-2},
  publisher = {{\'Ecole fran\c{c}aise de Rome}},
  issn = {0223-5102},
  doi = {10.4000/mefra.3475},
  urldate = {2021-07-27},
  abstract = {Questo intervento tenta di definire alcuni degli apporti che la ricerca bioarcheologica \`e in grado di fornire per la ricostruzione del paesaggio agrario di et\`a storica su scala regionale. Consapevoli di un necessario ampliamento dei campionamenti disponibili, crediamo anche che senza quell'approccio globale al paesaggio che da diversi anni il gruppo di ricerca dell'Universit\`a di Foggia tenta di strutturare e affermare, non pu\`o non risultare alquanto arduo il tentativo di definire dinamiche, modificazioni, persistenze di determinati fenomeni. L'obiettivo \`e quello di visualizzare gli elementi concreti su cui si impostarono le strutture economiche, le scelte politiche, la contingenza amministrativa. Al contempo si delineano la molteplicit\`a degli indirizzi di sviluppo di un territorio, le sue condivise o necessarie attivit\`a di trasformazione, sia nella sua componente ambientale che nelle sue modalit\`a di frequentazione antropica, le difficolt\`a della gestione e dell'utilizzo delle risorse.},
  copyright = {\textcopyright{} \'Ecole fran\c{c}aise de Rome},
  isbn = {9782728312153},
  langid = {italian},
  keywords = {allevamento,bioarcheologia,paesaggi agrari,Puglia settentrionale,transumanza.}
}


@book{greyConstructingCommunitiesLate2011,
  title = {Constructing {{Communities}} in the {{Late Roman Countryside}}},
  author = {Grey, Cam},
  year = {2011},
  publisher = {{Cambridge University Press}},
  address = {{Cambridge}},
  doi = {10.1017/CBO9780511994739},
  urldate = {2023-06-19},
  abstract = {This book is the first comprehensive treatment of the 'small politics' of rural communities in the Late Roman world. It places the diverse fates of those communities within a generalized model for exploring rural social systems. Fundamentally, social interactions in rural contexts in the period revolved around the desire of individual households to insure themselves against catastrophic subsistence failure and the need of the communities in which they lived to manage the attendant social tensions, inequalities and conflicts. A focus upon the politics of reputation in those communities provides a striking contrast to the picture painted by the legislation and the writings of Rome's literate elite: when viewed from the point of view of the peasantry, issues such as the Christianization of the countryside, the emergence of new types of patronage relations, and the effects of the new system of taxation upon rural social structures take on a different aspect.},
  isbn = {978-1-107-01162-5},
  file = {/Users/robertoragno/Downloads/Papers \: Articoli \: Libri accademici/Late Roman \: Early Middle Ages - gen folder/Grey 2011 - Constructing Communities in the Late Roman Countryside.pdf;/Users/robertoragno/Zotero/storage/MSE9ZJMI/C6163496D3B190945C200E2153141526.html}
}


